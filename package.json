    "@rollup/plugin-typescript": "^11.1.6",
    "@types/aws4": "^1.11.6",
    "@types/jest": "^29.5.12",
    "@types/nunjucks": "^3.2.6",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
